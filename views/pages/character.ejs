<% var t = type.toUpperCase() %>
<% var sp = "/" %>
<% var ds = "-" %>
<% if(t == "ALL" || t == "CHAR") { %>
    <% var gender = ["Male","Female"] %>
    <% var classes = ["Titan","Hunter","Warlock"] %>
    <% var item = JSON.parse(items[0]).Response.data.inventoryItem %>
    <%- gender[parseInt(characterBase.genderType)] %><%- sp %>
    <%- item.itemName %><%- sp %>
    <%- classes[parseInt(characterBase.classType)] %>
<% } %>
<% if(t == "ALL" || t == "STATS") { %>
    Light <%- characterBase.powerLevel %><%- ds %>
    Intellect <%- characterBase.stats.STAT_INTELLECT.value %>/300<%- ds %>
    Discipline <%- characterBase.stats.STAT_DISCIPLINE.value %>/300<%- ds %>
    Strength <%- characterBase.stats.STAT_STRENGTH.value %>/300
<% } %>
<% for(var i = 1; i < items.length; i++) { %>
    <% if(i <= 5 && (t == "ALL" || t == "ARMOR" || t == "LOADOUT")) { %>
        <% var item = JSON.parse(items[i]).Response.data.inventoryItem %>
        <%- item.tierTypeName %> <%- item.itemTypeName %> &quot;<%- item.itemName %>&quot;<%- sp %>
    <% } else if(i > 5 && i <= 8 && (t == "ALL" || t == "WEAPONS" || t == "LOADOUT")) { %>
        <% var item = JSON.parse(items[i]).Response.data.inventoryItem %>
        <%- item.tierTypeName %> <%- item.itemTypeName %> &quot;<%- item.itemName %>&quot;<%- sp %>
    <% } else if(i > 8 && t == "ALL") { %>
        <% var item = JSON.parse(items[i]).Response.data.inventoryItem %>
        <%- item.tierTypeName %> <%- item.itemTypeName %> <%- item.itemName %><%- sp %>
    <% } %>
<% } %>