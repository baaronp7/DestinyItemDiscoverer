<% var t = type.toUpperCase() %>
<% var sp = " " %>
<% if(t == "ALL" || t == "STATS") { %>
    <% var gender = ["Male","Female"] %>
    <% var classes = ["Titan","Hunter","Warlock"] %>
    <% var item = JSON.parse(items[0]).Response.data.inventoryItem %><%- sp %>
    <%- gender[parseInt(characterBase.genderType)] %><%- " " %>
    <%- item.itemName %><%- "\r\n" %>
    <%- classes[parseInt(characterBase.classType)] + "\r\n" %>
    Light <%- characterBase.powerLevel  + "\r\n" %>
    Intellect <%- characterBase.stats.STAT_INTELLECT.value %>/300<% "\r\n" %>
    Discipline <%- characterBase.stats.STAT_DISCIPLINE.value %>/300<% "\r\n" %>
    Strength <%- characterBase.stats.STAT_STRENGTH.value %>/300<% "\r\n" %>
<% } %>
<% for(var i = 1; i < items.length; i++) { %>
    <% if(i <= 5 && (t == "ALL" || t == "ARMOR" || t == "LOADOUT")) { %>
        <% var item = JSON.parse(items[i]).Response.data.inventoryItem %>
        <%- item.tierTypeName %> <%- item.itemTypeName %> &quot;<%- item.itemName %>&quot;<% "\r\n" %>
    <% } else if(i > 5 && i <= 8 && (t == "ALL" || t == "WEAPONS" || t == "LOADOUT")) { %>
        <% var item = JSON.parse(items[i]).Response.data.inventoryItem %>
        <%- item.tierTypeName %> <%- item.itemTypeName %> &quot;<%- item.itemName %>&quot;<% "\r\n" %>
    <% } else if(i > 8 && t == "ALL") { %>
        <% var item = JSON.parse(items[i]).Response.data.inventoryItem %>
        <%- item.tierTypeName %> <%- item.itemTypeName %> <%- item.itemName %><% "\r\n" %>
    <% } %>
<% } %>